{
  "name": "2019scape",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "imports": {
    "#jagex/*": "./src/jagex/*",
    "#lostcity/*": "./src/lostcity/*"
  },
  "scripts": {
    "start": "npm run ts-loader src/lostcity/app.ts",
    "web": "npm run ts-loader src/lostcity/web.ts",
    "lobby": "npm run ts-loader src/lostcity/lobby.ts",
    "world": "npm run ts-loader src/lostcity/world.ts",
    "dev": "concurrently \"nodemon src/lostcity/app.ts\" \"npm run cache:watch\"",
    "web:dev": "nodemon src/lostcity/web.ts",
    "lobby:dev": "nodemon src/lostcity/lobby.ts",
    "world:dev": "nodemon src/lostcity/world.ts",
    "test": "jest",
    "ts-loader": "node --no-warnings --loader ts-node/esm",
    "js5pack:openrs2": "npm run ts-loader src/lostcity/tools/js5pack/openrs2.ts",
    "js5pack:dat2": "npm run ts-loader src/lostcity/tools/js5pack/dat2.ts",
    "js5pack:dat2m": "npm run ts-loader src/lostcity/tools/js5pack/dat2m.ts",
    "js5pack:dat2split": "npm run ts-loader src/lostcity/tools/js5pack/dat2split.ts",
    "js5pack:flat": "npm run ts-loader src/lostcity/tools/js5pack/flat.ts",
    "cache:build": "npm run ts-loader src/lostcity/tools/pack/all.ts",
    "cache:verify": "npm run ts-loader src/lostcity/tools/js5pack/verify.ts",
    "cache:watch": "watch \"npm run cache:build\" ./data/src/scripts ./src/lostcity/tools"
  },
  "author": "Pazaz",
  "license": "MIT",
  "dependencies": {
    "@2004scape/rsmod-pathfinder": "^4.2.8",
    "@napi-rs/lzma": "^1.2.1",
    "dotenv": "^16.4.5",
    "fastify": "^4.26.2",
    "hash-wasm": "^4.11.0",
    "kysely": "^0.27.3",
    "mysql2": "^3.9.6",
    "seek-bzip": "^2.0.0",
    "tar": "^6.2.1"
  },
  "devDependencies": {
    "@swc/core": "^1.4.16",
    "@swc/helpers": "^0.5.10",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prisma": "^5.12.1",
    "prisma-kysely": "^1.8.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "watch": "^1.0.2"
  }
}
